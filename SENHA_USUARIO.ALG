Algoritmo "senha_usuario"

// Criação de senhas

Var

   senha, nsenha : inteiro

Inicio

   escreva("Digite a senha que deseja: ")
   leia(nsenha)
   escreval
   
   escreva("Digite a senha: ")
   leia(senha)

   enquanto senha <> nsenha faca
        escreva("Senha Invalida! Tente novamente: ")
        leia(senha)
   fimenquanto

   escreval
   escreval("Acesso permitido!")

Fimalgoritmo